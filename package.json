{
  "name": "my-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "autoprefixer": "^9.8.6",
    "bootstrap": "^5.1.0",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.6.5",
    "vue": "^2.6.14",
    "vue-awesome-swiper": "^4.1.1",
    "vue-svg-inline-loader": "^2.1.2",
    "vue2-smooth-scroll": "^1.5.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^6.0.1",
    "sass-loader": "^10.2.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "curly": [
        "error",
        "multi-line"
      ],
      "no-mixed-spaces-and-tabs": 0,
      "vue/max-attributes-per-line": [
        "error",
        {
          "singleline": 1,
          "multiline": 1
        }
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "no-console": "off",
      "no-undef": "off",
      "no-unused-vars": "warn",
      "indent": "off",
      "camelcase": [
        "off"
      ],
      "array-callback-return": "off",
      "no-return-assign": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
